{
  "name": "ng-ts-bp",
  "version": "1.0.0",
  "description": "angular and typescript boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "broccoli dist",
    "build:vendor": "rollup --config rollup.config.vendor.js",
    "watch": "broccoli-timepiece dist",
    "serve": "concurrently --kill-others \"npm run watch\" \"lite-server -c configs/bs-config.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run serve"
  },
  "author": "Zenon Zeniou",
  "license": "MIT",
  "devDependencies": {
    "broccoli": "^0.16.9",
    "broccoli-cli": "^1.0.0",
    "broccoli-es6modules": "^1.2.2",
    "broccoli-exec": "https://github.com/chriseldredge/broccoli-exec/tarball/0.2.1",
    "broccoli-funnel": "^1.0.1",
    "broccoli-merge-trees": "^1.1.1",
    "broccoli-rollup": "0.0.4",
    "broccoli-sass": "^0.7.0",
    "broccoli-source": "^1.1.0",
    "broccoli-timepiece": "^0.3.0",
    "broccoli-tslinter": "^0.1.0",
    "broccoli-typescript-compiler": "^0.6.0",
    "concurrently": "^2.1.0",
    "lite-server": "^2.2.0",
    "rollup": "^0.26.3",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-includepaths": "^0.1.5",
    "rollup-plugin-node-resolve": "^1.5.0",
    "rollup-plugin-typescript": "^0.7.5",
    "rollup-plugin-uglify": "^0.3.1",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  },
  "dependencies": {
    "angular": "^1.5.6",
    "angular-animate": "^1.5.6",
    "angular-aria": "^1.5.6",
    "angular-material": "^1.1.0-rc4-master-06e7e99",
    "angular-ui-router": "^1.0.0-alpha.5"
  }
}
